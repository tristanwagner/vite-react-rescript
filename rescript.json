{
  "name": "vite-react-rescript-starter",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "uncurried": false,
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-json-combinators",
    "@glennsl/rescript-fetch"
  ],
  "bsc-flags": ["-open RescriptCore", "-open JsonCombinators"],
  "bs-dev-dependencies": ["rescript-vitest"]
}
